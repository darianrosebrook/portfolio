      },
      "button": {
        "01": { "$type": "dimension", "$value": "{core.text.size.5}" },
        "02": { "$type": "dimension", "$value": "{core.text.size.4}" },
        "03": { "$type": "dimension", "$value": "{core.text.size.3}" }
      },
      "fontWeight": {
        "light": { "$type": "number", "$value": 300 },
        "regular": { "$type": "number", "$value": 400 },
        "medium": { "$type": "number", "$value": 500 },
        "bold": { "$type": "number", "$value": 700 },
        "black": { "$type": "number", "$value": 900 }
      },
      "lineHeight": {
        "collapse": { "$type": "number", "$value": 1 },
        "body": { "$type": "number", "$value": 1.5 },
        "heading": {
          "$type": "number",
          "$value": "{semantic.typography.lineHeight.collapse}"
        }
      },
      "letterSpacing": {
        "tight": { "$type": "dimension", "$value": "-0.018em" },
        "normal": { "$type": "dimension", "$value": "0" },
        "wide": { "$type": "dimension", "$value": "0.018em" }
      }
    },
    "size": {
      "tapTarget": {
        "large": { "$type": "dimension", "$value": "{core.size.09}" },
        "medium": { "$type": "dimension", "$value": "{core.size.08}" },
        "small": { "$type": "dimension", "$value": "{core.size.07}" }
      },
      "border": {
        "thin": { "$type": "dimension", "$value": "{core.size.01}" },
        "default": { "$type": "dimension", "$value": "{core.size.02}" },
        "bold": { "$type": "dimension", "$value": "{core.size.03}" },
        "offset": { "$type": "dimension", "$value": "{core.size.02}" }
      },
      "radius": {
        "00": { "$type": "dimension", "$value": "{core.size.00}" },
