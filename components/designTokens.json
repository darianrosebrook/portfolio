                "$value": "0.25vh"
              },
              "vw": {
                "$type": "dimension",
                "$value": "0.25vw"
              }
            }
          }
        }
      },
      "body": {
        "01": { "$type": "dimension", "$value": "{core.text.size.5}" },
        "02": { "$type": "dimension", "$value": "{core.text.size.4}" },
        "03": { "$type": "dimension", "$value": "{core.text.size.3}" },
