{
  "prefix": "blockquote",
  "tokens": {
    "color": {
      "foreground": {
        "primary": "{semantic.color.foreground.secondary}"
      },
      "background": {
        "default": "{semantic.color.background.secondary}"
      },
      "border": {
        "default": "{semantic.color.border.accent}",
        "accent": "{semantic.color.border.accent}"
      }
    },
    "typography": {
      "fontStyle": "italic",
      "fontWeight": "{semantic.typography.fontWeight.medium}",
      "sm": {
        "fontSize": "{semantic.typography.body.03}",
        "lineHeight": "{semantic.typography.lineHeight.body}"
      },
      "md": {
        "fontSize": "{semantic.typography.body.02}",
        "lineHeight": "{semantic.typography.lineHeight.body}"
      },
      "lg": {
        "fontSize": "{semantic.typography.body.01}",
        "lineHeight": "{semantic.typography.lineHeight.body}"
      }
    },
    "size": {
      "padding": {
        "default": "{core.spacing.size.05}"
      },
      "radius": {
        "default": "{core.shape.radius.medium}"
      },
      "border": {
        "thick": "{core.shape.border.width.thick}"
      }
    }
  }
}
