{
  "$schema": "../designTokens.schema.json",
  "component": {
    "pagination": {
      "gap": {
        "x": {
          "$type": "dimension",
          "$value": "{spacing.size.02}"
        }
      },
      "minSize": {
        "$type": "dimension",
        "$value": "32px"
      }
    },
    "foreground": {
      "default": {
        "$type": "color",
        "$value": "{color.mode.dark}"
      }
    },
    "typography": {
      "variation": {
        "wghtHeading": {
          "$type": "number",
          "$value": 700
        },
        "wdthResponsive": {
          "$type": "number",
          "$value": 100
        }
      }
    },
    "otp": {
      "field": {
        "gap": {
          "$type": "dimension",
          "$value": "{spacing.size.02}"
        },
        "minWidth": {
          "$type": "dimension",
          "$value": "48px"
        },
        "height": {
          "$type": "dimension",
          "$value": "48px"
        },
        "fontSize": {
          "$type": "dimension",
          "$value": "{typography.ramp.4}"
        },
        "fontWeight": {
          "$type": "fontWeight",
          "$value": "{typography.fontWeight.medium}"
        },
        "radius": {
          "$type": "borderRadius",
          "$value": "{shape.radius.02}"
        }
      },
      "color": {
        "text": {
          "$type": "color",
          "$value": "{color.palette.neutral.600}"
        },
        "bg": {
          "$type": "color",
          "$value": "{color.palette.neutral.100}"
        },
        "border": {
          "$type": "color",
          "$value": "{color.palette.neutral.200}"
        },
        "focus": {
          "$type": "color",
          "$value": "{color.palette.red.500}"
        }
      }
    },
    "test": {
      "dsSemanticColorBgPrimary": {
        "$type": "color",
        "$value": "{color.palette.neutral.100}"
      },
      "customSemanticColorBgPrimary": {
        "$type": "color",
        "$value": "{color.palette.neutral.100}"
      },
      "myCustomBg": {
        "$type": "color",
        "$value": "#007bff"
      },
      "path": {
        "$type": "string",
        "$value": "test-path"
      },
      "typeWghtHeading": {
        "$type": "number",
        "$value": 700
      },
      "typeWdthResponsive": {
        "$type": "number",
        "$value": 100
      }
    }
  }
}
