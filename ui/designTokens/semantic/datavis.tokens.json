{
  "$schema": "../designTokens.schema.json",
  "datavis": {
    "onFill": {
      "default": {
        "$type": "color",
        "$value": "{color.mode.white}"
      },
      "muted": {
        "$type": "color",
        "$value": "{color.palette.neutral.100}"
      }
    },
    "gridline": {
      "major": {
        "$type": "color",
        "$value": "{color.palette.neutral.300}"
      },
      "minor": {
        "$type": "color",
        "$value": "{color.palette.neutral.200}"
      }
    },
    "strokeWidth": {
      "series": {
        "$type": "dimension",
        "$value": {
          "value": 2,
          "unit": "px"
        }
      },
      "grid": {
        "$type": "dimension",
        "$value": {
          "value": 1,
          "unit": "px"
        }
      }
    }
  }
}
