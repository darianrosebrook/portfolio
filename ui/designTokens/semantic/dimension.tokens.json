{
  "$schema": "../designTokens.schema.json",
  "dimension": {
    "minTarget": {
      "$type": "dimension",
      "$value": "{dimension.tapTargetMin}"
    },
    "tapTarget": {
      "min": {
        "$type": "dimension",
        "$value": "{dimension.tapTargetMin}"
      },
      "large": {
        "$type": "dimension",
        "$value": {
          "value": 48,
          "unit": "px"
        },
        "$description": "Large tap target size for better accessibility"
      }
    },
    "buttonMinHeight": {
      "$type": "dimension",
      "$value": "{dimension.actionMinHeight}"
    },
    "breakpoint": {
      "sm": {
        "$type": "dimension",
        "$value": {
          "value": 640,
          "unit": "px"
        }
      },
      "md": {
        "$type": "dimension",
        "$value": {
          "value": 768,
          "unit": "px"
        }
      },
      "lg": {
        "$type": "dimension",
        "$value": {
          "value": 1024,
          "unit": "px"
        }
      },
      "xl": {
        "$type": "dimension",
        "$value": {
          "value": 1280,
          "unit": "px"
        }
      }
    }
  }
}
