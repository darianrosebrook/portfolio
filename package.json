{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "npm run tokens:build && npm run typecheck && next build",
    "build:check": "npm run tokens:build && stylelint --allow-empty-input \"**/*.{css,scss}\" && prettier --check . && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:fix:css": "stylelint --fix \"**/*.{css,scss}\"",
    "typecheck": "tsc --noEmit",
    "validate": "bash scripts/validate-build.sh",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:update": "playwright test --update-snapshots",
    "test:e2e:report": "playwright show-report",
    "analyze": "ANALYZE=true npm run build",
    "tokens:build": "tsx utils/designTokens/runners/build.ts",
    "tokens:build:full": "tsx utils/designTokens/runners/build.ts --no-incremental",
    "tokens:compose": "tsx utils/designTokens/generators/compose.ts",
    "tokens:global": "tsx utils/designTokens/generators/global.ts",
    "tokens:types": "tsx utils/designTokens/generators/types.ts",
    "tokens:scss": "node utils/designTokens/generators/generateCSSTokens.mjs",
    "tokens:schema": "node utils/designTokens/generators/generateSchema.mjs",
    "tokens:validate": "node utils/designTokens/validators/validateTokens.mjs",
    "tokens:inspect": "node utils/designTokens/validators/inspectTokens.mjs",
    "tokens:usage": "tsx utils/designTokens/runners/usage.ts",
    "tokens:usage:json": "tsx utils/designTokens/runners/usage.ts --json",
    "validate:components": "node scripts/validateComponents.mjs",
    "validate:component-visibility": "node scripts/validateComponentVisibility.mjs",
    "scaffold:component": "node scripts/scaffoldComponent.mjs",
    "generate:components-docs": "node scripts/generateComponentsAndDocs.mjs",
    "generate:contracts": "node scripts/generateContracts.mjs",
    "components:setup": "npm run generate:contracts && npm run validate:components",
    "components:health": "node scripts/componentHealthCheck.mjs",
    "setup:hooks": "bash scripts/setup-hooks.sh",
    "hook:audit": "bash scripts/audit-hook-bypasses.sh",
    "perf:budgets": "lhci autorun --config=.lighthouserc.js",
    "perf:budgets:mobile": "lhci autorun --config=lighthouserc.mobile.js",
    "perf:audit": "lhci autorun --config=.lighthouserc.js --collect.url=http://localhost:3000",
    "perf:audit:all": "npm run build && npm run start & sleep 10 && npm run perf:budgets && pkill -f 'next start'",
    "perf:compare": "lhci autorun --config=.lighthouserc.js --collect.numberOfRuns=1"
  },
  "dependencies": {
    "@atproto/api": "^0.16.9",
    "@awesome.me/kit-0ba7f5fefb": "^1.0.4",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@gsap/react": "^2.1.2",
    "@heroicons/react": "^2.2.0",
    "@supabase/ssr": "latest",
    "@supabase/supabase-js": "latest",
    "@tiptap/core": "^3.4.4",
    "@tiptap/extension-bubble-menu": "^3.4.4",
    "@tiptap/extension-character-count": "^3.4.4",
    "@tiptap/extension-code-block": "^3.4.4",
    "@tiptap/extension-code-block-lowlight": "^3.4.4",
    "@tiptap/extension-color": "^3.4.4",
    "@tiptap/extension-document": "^3.4.4",
    "@tiptap/extension-dropcursor": "^3.4.4",
    "@tiptap/extension-focus": "^3.4.4",
    "@tiptap/extension-heading": "^3.4.4",
    "@tiptap/extension-highlight": "^3.4.4",
    "@tiptap/extension-horizontal-rule": "^3.4.4",
    "@tiptap/extension-image": "^3.4.4",
    "@tiptap/extension-link": "^3.4.4",
    "@tiptap/extension-paragraph": "^3.4.4",
    "@tiptap/extension-placeholder": "^3.4.4",
    "@tiptap/extension-table": "^3.4.4",
    "@tiptap/extension-table-cell": "^3.4.4",
    "@tiptap/extension-table-header": "^3.4.4",
    "@tiptap/extension-table-row": "^3.4.4",
    "@tiptap/extension-task-item": "^3.4.4",
    "@tiptap/extension-task-list": "^3.4.4",
    "@tiptap/extension-text": "^3.4.4",
    "@tiptap/extension-text-align": "^3.4.4",
    "@tiptap/extension-text-style": "^3.4.4",
    "@tiptap/extension-underline": "^3.4.4",
    "@tiptap/html": "^3.4.4",
    "@tiptap/pm": "^3.4.4",
    "@tiptap/react": "^3.4.4",
    "@tiptap/starter-kit": "^3.4.4",
    "@tiptap/suggestion": "^3.4.4",
    "@types/hls.js": "^0.13.3",
    "@vercel/analytics": "^1.5.0",
    "bezier-js": "^6.1.4",
    "eslint-plugin-prettier": "^5.5.4",
    "fontkit": "^2.0.4",
    "gsap": "^3.13.0",
    "highlight.js": "^11.11.1",
    "hls.js": "^1.6.12",
    "kld-intersections": "^0.7.0",
    "lowlight": "^3.3.0",
    "next": "15.5.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "svg-intersections": "^0.4.0",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@next/bundle-analyzer": "^15.5.3",
    "@playwright/test": "^1.55.1",
    "@stylistic/stylelint-plugin": "^3.1.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.19.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/svg-intersections": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "glob": "^11.0.3",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "stylelint": "^16.24.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
